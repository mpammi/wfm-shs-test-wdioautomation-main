{
  "compilerOptions": {
    "types": [
      "node",
      "expect-webdriverio",
      "chai",
      "@wdio/cucumber-framework",
      "@wdio/globals/types",
    ],
    //"typeRoots": ["./node_modules/@types", "./types"],
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "noEmit": true,
    "allowImportingTsExtensions": true
  }
}
